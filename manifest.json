{
    "manifest_version": 3,
    "name": "Find&Highlight",
    "version": "0.1.0",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["script.js"],
        "css": ["style.css"]
      }
    ],
    "icons": {
      "16": "/Icon/highlighter16.png",
      "32": "/Icon/highlighter32.png",
      "64": "/Icon/highlighter64.png",
      "128": "/Icon/highlighter128.png"
    }
  }
  